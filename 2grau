programa
{
	
inclua biblioteca Matematica --> mat
	funcao inicio()
	{
		real a,b,c, delta, x1, x2, raizdelta
		escreva("Entre o valor A: ")
		leia(a)
		escreva("Entre o valor B: ")
		leia(b)
		escreva("Entre o valor C: ")
		leia(c)
		delta = (b*b)*(-4*a*c)
		raizdelta = mat.raiz(delta, 2.0)
		x1 = (-b+raizdelta)/(2*a)
		x2 = (-b-raizdelta)/(2*a)
		escreva("O x1 é ", x1)
		escreva("O x2 é ", x2)
	
		
	}
}
